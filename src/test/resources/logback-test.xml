<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-4relative [%thread] %highlight(%-5level) %logger{35} - %msg %ex %mdc %n</pattern>
    </encoder>
  </appender>

  <root level="${rootLevel:-DEBUG}">
    <appender-ref ref="console"/>
  </root>
</configuration>
